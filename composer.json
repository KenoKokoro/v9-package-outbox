{
    "name": "kenokokoro/v9-package-outbox",
    "description": "Outbox helper for Laravel/Lumen",
    "keywords": [
        "framework",
        "laravel",
        "lumen",
        "json",
        "response"
    ],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=7.4.0",
        "illuminate/database": "^6.3",
        "illuminate/container": "^6.3",
        "illuminate/support": "^6.3",
        "illuminate/console": "^6.3",
        "kenokokoro/v9-package-dal": "^0.1",
        "illuminate/log": "^6.3",
        "illuminate/encryption": "^6.3"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "V9\\Outbox\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "V9\\Tests\\Outbox\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "V9\\Outbox\\Providers\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
