{
    "name": "kenokokoro/v9-response",
    "description": "Json response builder for Laravel/Lumen",
    "keywords": [
        "framework",
        "laravel",
        "lumen",
        "json",
        "response"
    ],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=7.4.0",
        "illuminate/database": "^6.1",
        "illuminate/container": "^6.1",
        "illuminate/support": "^6.1"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "V9\\Outbox\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "V9\\Tests\\Outbox\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
